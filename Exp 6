#include <stdio.h>

struct Node {
    char data;
    struct Node* next;
};

int isPalindrome(struct Node* head) {
    struct Node* slow = head;
    struct Node* fast = head;
    struct Node* prev = NULL;
    struct Node* temp;

    while (fast != NULL && fast->next != NULL) {
        fast = fast->next->next;
        temp = slow->next;
        slow->next = prev;
        prev = slow;
        slow = temp;
    }

    if (fast != NULL) {
        slow = slow->next;
    }

    while (prev != NULL && slow != NULL) {
        if (prev->data != slow->data)
            return 0;
        prev = prev->next;
        slow = slow->next;
    }

    return 1;
}

int main() {
    struct Node n1, n2, n3, n4, n5;

    n1.data = 'R'; n1.next = &n2;
    n2.data = 'A'; n2.next = &n3;
    n3.data = 'D'; n3.next = &n4;
    n4.data = 'A'; n4.next = &n5;
    n5.data = 'R'; n5.next = NULL;

    if (isPalindrome(&n1))
        printf("Yes\n");
    else
        printf("No\n");

    n1.data = 'C'; n1.next = &n2;
    n2.data = 'O'; n2.next = &n3;
    n3.data = 'D'; n3.next = &n4;
    n4.data = 'E'; n4.next = NULL;

    if (isPalindrome(&n1))
        printf("Yes\n");
    else
        printf("No\n");

    return 0;
}
