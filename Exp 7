#include <stdio.h>
#include <string.h>

int main() {
    char s[] = "tree";
    int freq[256] = {0};
    int len = strlen(s);

    for (int i = 0; i < len; i++) {
        freq[(int)s[i]]++;
    }

    for (int i = 0; i < len; i++) {
        if (freq[(int)s[i]] != 0) {
            printf("%c->%d", s[i], freq[(int)s[i]]);
            freq[(int)s[i]] = 0;
            if (i < len - 1) printf(", ");
        }
    }

    return 0;
}
