#include <stdio.h>

int fib(int n) {
    if (n == 0)
        return 0;
    if (n == 1)
        return 1;
    return fib(n - 1) + fib(n - 2);
}

int main() {
    int n = 10;
    int sum = 0;

    printf("Fibonacci series\n");

    for (int i = 0; i < n; i++) {
        int value = fib(i);
        sum += value;
        printf("%d", value);
        if (i < n - 1)
            printf(", ");
    }

    printf("\n\nSum: %d", sum);

    return 0;
}
